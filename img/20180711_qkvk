<div class="spacer c_spacer"></div><div id='component_5725847'></div><div      component_map_id="5725847" >
    <script>
var oPs = document.getElementById("LeftTime");
function FreshTime(){
         var endtime=new Date("2018/07/12,00:00:00");//开始时间
         var nowtime = new Date();//当前时间
         var lefttime= parseInt((endtime.getTime() - nowtime.getTime()) / 1000),
                   d =  parseInt(lefttime/(24*60*60)),
                              h =  parseInt(lefttime/(60*60)%24),
                              m =  parseInt(lefttime/60%60),
                              s =  parseInt(lefttime%60);
         if(h <= 9){h = '0' + h};
         if(m <= 9){m = '0' + m};     
         if(s <= 9){s = '0' + s};
         if(d <= 0)  {
                   oPs.innerHTML = "<span>距离活动开始仅剩：" + h + "：" + m + "：" + s + "";
         }else{
                   oPs.innerHTML = "<span>距离活动开始仅剩：" + d + "天" + h + "小时" + m + "分" + s + "秒</span>";
         }
         if(lefttime<=0){
                   oPs.style.display = 'none';
         }
}
FreshTime();
var sh = setInterval(FreshTime,500);
</script> 
</div>
